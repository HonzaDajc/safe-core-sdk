{
  "name": "@gnosis.pm/safe-ethers-adapters",
  "version": "0.1.0-alpha.9",
  "description": "Safe Ethers Adapters",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "keywords": [
    "Ethereum",
    "Gnosis",
    "Safe",
    "Ethers"
  ],
  "scripts": {
    "unbuild": "rimraf dist *.tsbuildinfo",
    "build": "tsc",
    "test": "nyc mocha -r ts-node/register tests/**/*.test.ts",
    "example:creation": "ts-node examples/creation.ts",
    "example:interaction": "ts-node examples/interaction.ts",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "format": "prettier --write \"{src,tests}/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepublish": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/safe-global/safe-core-sdk.git"
  },
  "author": "Gnosis Safe (https://gnosis-safe.io)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/safe-global/safe-core-sdk/issues"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/safe-global/safe-core-sdk#readme",
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.1.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ethers": "^5.6.8",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "lint-staged": {
    "src/**/!(*test).ts": [
      "yarn lint",
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "peerDependencies": {
    "@ethersproject/abstract-provider": "^5.5.1",
    "@ethersproject/abstract-signer": "^5.5.0",
    "@ethersproject/bignumber": "^5.5.0",
    "@ethersproject/properties": "^5.5.0"
  },
  "dependencies": {
    "@gnosis.pm/safe-core-sdk": "^2.1.0",
    "@gnosis.pm/safe-core-sdk-types": "^1.1.0",
    "@gnosis.pm/safe-deployments": "^1.14.0",
    "axios": "^0.27.2"
  }
}
